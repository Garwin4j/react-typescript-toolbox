{
    "name": "react-typescript-toolbox",
    "displayName": "React Typescript Toolbox",
    "description": "Helpful toolbox for React Web Applications with Typescript",
    "version": "0.5.0",
    "publisher": "svenliebig",
    "repository": {
        "url": "https://github.com/Sly321/react-typescript-toolbox"
    },
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:reactTypeScriptToolbox.generateComponent"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "reactTypeScriptToolbox.generateComponent",
                "category": "ReactTS Toolbox",
                "title": "Generate Component",
                "icon": "images/logo.svg"
            }
        ],
        "menus": {
            "explorer/context": [{
                "command": "reactTypeScriptToolbox.generateComponent",
                "group": "edit"
            }]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "minor": "vsce publish minor",
        "patch": "vsce publish patch",
        "major": "vsce publish major"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "vsce": "^1.30.0"
    }
}